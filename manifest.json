{
  "manifest_version": 3,
  "name": "Codeforces Problem Tracker",
  "version": "1.0",
  "description": "Track solved and unsolved problems on Codeforces problemset",
  "permissions": ["storage"],
  "host_permissions": ["https://codeforces.com/*"],
  "content_scripts": [
    {
      "matches": ["https://codeforces.com/problemset*"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ]
}
